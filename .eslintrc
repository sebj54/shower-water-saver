{
  "extends": "airbnb",
  "rules": {
      "indent": [2, 4],
      "max-len": [2, {"code": 120, "tabWidth": 4, "ignoreUrls": true}],
      "func-names": [0],
      "arrow-body-style": [0],
      "no-param-reassign": [0],
      "react/jsx-indent-props": [2, 4],
      "import/no-extraneous-dependencies": [0]
  },
  "globals": {
      "document": true,
  }
}
